
<div class="team">
  <h1>Team Name: <%= @team.name %></h1>
  <h4>Team Motto: <%= @team.motto %></h4>
</div><br>
 
<h1>Members</h1>
<% @heros.each do |super_hero| %>
  <div class="super_hero">
    <p>Hero Name: <%= super_hero.name %></p>
    <p>Hero Power: <%= super_hero.power %></p>
    <p>Hero Biography: <%= super_hero.biography %></p>
  </div><br>
<% end %>